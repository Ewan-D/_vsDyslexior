---
layout: layouts/page
title: 'all tags'
---

      <div class="inline" aria-labelledby="taglist">
      <h3 class="hidden" id="tagList">tags:</h3>
      <ul class="inline">

          {% for tag in collections.allTags | tagFilter('post') %}
              <li class="inline px-1 pt-1 bg-gray-200 eleventyTag">
                  {% set tagUrl %}/tags/{{ tag | slug }}/{% endset %}
                  <a href="{{ tagUrl | url }}">{{ tag }}</a>
              </li>
          {% endfor %}
      </ul>
      </div>
